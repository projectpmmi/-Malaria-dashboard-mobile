{
  "expo": {
    "name": "Malaria Dashboard Madagascar",
    "slug": "MalariaDashboardMobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.malaria.dashboard",
      "versionCode": 7
    },
    "web": {
      "favicon": "./assets/logo-favicon.png"
    }
  }
}
